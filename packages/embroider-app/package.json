	"homepage": "https://github.com/twyr/embroider-template/tree/main/packages/embroider-app",
	"license": "MITNFA",
	"readme": "README.md",
	"version": "1.0.2",
	"keywords": [
		"ember",
		"embroider"
	],
	"author": {
		"name": "Vish Desai",
